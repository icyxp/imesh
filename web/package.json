{
  "name": "imesh-web",
  "version": "1.0.0",
  "description": "imesh web",
  "author": "icyboy <xupeng.js@gmail.com>",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "eslint src --ext .js --ext .jsx --fix",
    "copy:fonts": "cpx \"./node_modules/source-sans-pro/**/*\" ./dist/fonts",
    "copy:json": "cpx \"./src/*.json\" ./dist",
    "dev-server": "webpack serve --host=0.0.0.0 --content-base dist/ --history-api-fallback",
    "dev": "webpack serve --hot",
    "validate": "npm ls",
    "dist": "npm run clean && npm run copy:fonts && npm run copy:json && webpack",
    "start": "npm run dist && node server.js"
  },
  "engines": {
    "node": ">=6.0"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "express": "^4.16.2",
