    "keymirror": "^0.1.1",
    "keypress.js": "2.1.0-1",
    "lodash": "^4.17.10",
    "numeral": "^1.5.3",
    "prop-types": "^15.8.0",
    "query-string": "^4.2.3",
    "react": "^16.0.0",
    "react-addons-shallow-compare": "^15.3.2",
    "react-bootstrap": "^1.0.0",
    "react-checkbox-group": "^3.3.1",
    "react-dom": "^16.0.0",
    "react-highcharts": "^12.0.0",
    "react-virtualized": "^9.0.0",
    "socket.io-client": "^1.5.1",
